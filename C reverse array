#include <stdio.h>

int main() {
    int size, i;

    printf("Enter the size of the array: ");
    scanf("%d", &size);

    int array[size];

    printf("Enter %d elements:\n", size);
    for (i = 0; i < size; i++) {
        scanf("%d", &array[i]);
    }

    printf("Array elements in reverse order:\n");
    for (i = size - 1; i >= 0; i--) {
        printf("%d ", array[i]);
    }

    return 0;
}
